{
	"schemaVersion": 1,
	"id": "squirmy_wormy",
	"version": "${version}",
	"name": "Squirmy Wormy",
	"description": "A Minecraft mod that gives you worms",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/squirmy_wormy/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.example.Worms",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.example.WormsClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.example.WormsDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"worms.mixins.json",
		{
			"config": "worms.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}